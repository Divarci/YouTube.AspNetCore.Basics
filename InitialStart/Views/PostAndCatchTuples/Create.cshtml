@using InitialStart.Controllers
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model (TestObjectOne objectOne,TestObjectTwo objectTwo)

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<form asp-action="Create" method="post">
    <input type="text" asp-for="objectOne.Name" placeholder="Object One" />
    <br/>
    <input type="text" asp-for="objectTwo.Address" placeholder="Object Two" />
    <br/>
    <button>Send</button>
</form>


